//Este algoritmo lê 5 números e imprime o maior e o menor número digitado.

#include <stdio.h>

int main()
{
    int num, maiorNum=0, menorNum=0, i=1;
    while(i<=5){
        printf("Digite um numero inteiro: \n");
        scanf("%d", &num);
        if(num>=maiorNum){
            maiorNum=num;
        }
        else{
            menorNum=num;
        }
        i++;
    }
    printf("O maior numero digitado foi %d e o menor numero digitado foi %d", maiorNum, menorNum);
    return 0;
}
