#include <stdio.h>

int main()
{
    int a, b, c;
    printf("Digite respectivamente, numeros A, B, C diferentes:");
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    
    printf("O valor de A é: %d, o valor de B é: %d e o valor de C é %d \n", a, b, c);
    if(a>b){
        if(b>c){
            printf("%d>%d>%d", a, b, c);
        }
        else{
            if(a>c){
                printf("%d>%d>%d", a, c, b);
            }
            else{
                printf("%d>%d>%d", c, a, b);
            }
        }
    }
    else if(b>a){
        if(a>c){
            printf("%d>%d>%d", b, a, c);
        }
        else{
            if(b>c){
                printf("%d>%d>%d", b, c, a);
            }
            else{
                printf("%d>%d>%d", c, b, a);
            }
        }
    }
return 0;
}
