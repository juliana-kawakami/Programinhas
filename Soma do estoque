#include <stdio.h>

//Programa que armazena a quantidade de produtos em um estoque
//(produto 0, produto 1, ...)
//e fornece a quantidade de produtos no estoque.

int main()
{
    int produtos[9];
    int soma;
    for(int i = 0, soma = 0;i <= 9; i++){
        printf("Digite a quantidade de produtos no estoque:\n");
        scanf("%d", &produtos[i]);
        printf("O valor digitado foi %d \n", produtos[i]);
        soma = soma + produtos[i];
    }
    printf("A soma dos produtos do estoque deu %d", soma);

    return 0;
}
